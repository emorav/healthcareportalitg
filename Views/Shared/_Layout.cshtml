                            <li><a href="../HealthCare/BotMeeting">Book Meeting with Bot</a></li>
                            <li><a href="../HealthCare/Configuration">Configuration</a></li>
                            @*<li><a href="../HealthCare/Configuration">Conference</a></li>*@
                            @*<li><a href="#">Sign Out</a></li>*@

                        </ul>
                    </div><!-- /.navbar-collapse -->
                </div><!-- /.container-fluid -->
